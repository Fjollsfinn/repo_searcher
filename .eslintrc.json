{
    "env": {
      "browser": true,
      "node": true,
      "jest": true
    },
    "extends": "airbnb",
    "parser": "babel-eslint",
    "plugins": [
      "react",
      "react-hooks"
    ],
    "settings": {
      "import/resolver": {
        "babel-module": {}
      }
    },
    "rules": {
      "template-curly-spacing" : "off",
      "indent": "off",
      "react-hooks/rules-of-hooks": "error",
      "react-hooks/exhaustive-deps": "error"
    }
  }
  